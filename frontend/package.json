{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\"",
    "preview": "vite preview",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "preinstall": "node -e \"if (process.arch === 'arm64') { console.log('Installing arm64-specific dev dependencies...'); require('child_process').execSync('npm install @rollup/rollup-linux-arm64-gnu --save-dev --no-save', {stdio: 'inherit'}); }\""
  },
  "dependencies": {
    "@cubing/icons": "^1.1.2",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@tanstack/react-table": "^8.20.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@wca/helpers": "^1.1.5",
    "@weston/react-world-flags": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^11.5.4",
    "input-otp": "^1.4.1",
    "jotai": "^2.10.1",
    "lucide-react": "^0.469.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.26.1",
    "react-spinners": "^0.15.0",
    "recharts": "^2.15.0",
    "scramble-display": "^0.5.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "wcif-helpers": "^0.1.9",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-table": "^7.7.20",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
