    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.4",
    "vite": "^5.4.10",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^1.5.0"
  }
